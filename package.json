{
	"name": "medikit",
	"main": "expo-router/entry",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"start": "expo start",
		"android": "expo run:android --device",
		"ios": "expo run:ios",
		"web": "expo start --web",
		"prebuild": "expo prebuild",
		"lint": "expo lint",
		"build": "eas build",
		"build:apk": "eas build -p android --profile apk",
		"jobs": "adb shell dumpsys jobscheduler | grep -A 40 -m 1 -E \"JOB #.* com.rkanik.medikit\"",
		"job": "adb shell cmd jobscheduler run -f com.rkanik.medikit"
	},
