{
	"expo": {
		"name": "Medi Kit",
		"slug": "medikit",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "medikit",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true,
			"infoPlist": {
				"NSPhotoLibraryUsageDescription": "Medi Kit needs access to your photo library so you can attach prescriptions and receipts."
			}
		},
		"android": {
			"adaptiveIcon": {
				"backgroundColor": "#E6F4FE",
				"foregroundImage": "./assets/images/android-icon-foreground.png",
				"backgroundImage": "./assets/images/android-icon-background.png",
				"monochromeImage": "./assets/images/android-icon-monochrome.png"
			},
			"edgeToEdgeEnabled": true,
			"predictiveBackGestureEnabled": false,
			"package": "com.rkanik.medikit",
			"googleServicesFile": "./google-services.json"
		},
		"web": {
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			"@react-native-google-signin/google-signin",
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/splash-icon.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#ffffff",
					"dark": {
						"backgroundColor": "#0a0a0a"
					}
				}
			],
			[
				"expo-image-picker",
				{
					"photosPermission": "Medi Kit needs access to your photo library so you can attach prescriptions and receipts.",
					"cameraPermission": "Medi Kit needs access to your camera so you can take photos of your prescriptions and receipts."
				}
			]
		],
		"experiments": {
			"typedRoutes": true,
			"reactCompiler": true
		},
		"extra": {
			"router": {},
			"eas": {
				"projectId": "0cdbdddc-655f-4eb8-9f54-23d0651ae1fd"
			}
		},
		"owner": "rkanik"
	}
}
